{
  "name": "cumulocity-asset-viewer-widget",
  "version": "1.0.5",
  "description": "The Asset Viewer Widget help you to display asset/device data in Tile/List view, along with navigation to template dashboards(App Builder only).",
  "author": "Darpankumar Lalani - Software AG, Global Presales",
  "license": "Apache 2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildPatch": "cd projects/gp-asset-viewer && npm version patch && ng build gp-asset-viewer && cd ../../ && cd ./dist/gp-asset-viewer && npm pack && move *.tgz ../",
    "buildMinor": "cd projects/gp-asset-viewer && npm version minor && ng build gp-asset-viewer && cd ../../ && cd ./dist/gp-asset-viewer && npm pack && move *.tgz ../",
    "buildMajor": "cd projects/gp-asset-viewer && npm version major && ng build gp-asset-viewer && cd ../../ && cd ./dist/gp-asset-viewer && npm pack && move *.tgz ../",
    "serve": "ng build gp-asset-viewer && npm i dist/gp-asset-viewer && ng s",
    "runtime": "gulp --gulpfile ./runtime/gulpfile.js"
  },
  "private": true,
